{
    "agent": {
            "run_as_user": "root"
    },
    "logs": {
            "logs_collected": {
                    "files": {
                            "collect_list": [
                                    {
                                            "file_path": "/app/log/log.log",
                                            "log_group_class": "STANDARD",
                                            "log_group_name": "log.log",
                                            "log_stream_name": "{instance_id}",
                                            "retention_in_days": -1
                                    }
                            ]
                    }
            }
    }
}